{
  "name": "newdeblok",
  "module": "server/index.ts",
  "scripts": {
    "preinstall": "npx only-allow bun",
    "watch": "tailwindcss -i static/tw.css -o static/app.css -m -w",
    "build": "tailwindcss -i static/tw.css -o static/app.css -m",
    "start": "bash scripts/unattended.sh",
    "dev": "concurrently \"bun run watch\" \"bun run start\"",
    "deploy": "bash scripts/deploy.sh",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "type": "module",
  "dependencies": {
    "@catppuccin/tailwindcss": "^0.1.6",
    "@elysiajs/cors": "^0.8.0",
    "@elysiajs/static": "^0.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "elysia": "^0.8.15",
    "elysia-rate-limit": "^2.0.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "flowbite": "^2.2.1",
    "http-proxy": "^1.18.1",
    "node-http-proxy": "^0.2.4",
    "prettier": "^3.2.5",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.1",
    "yaml": "2.2.2"
  }
}
